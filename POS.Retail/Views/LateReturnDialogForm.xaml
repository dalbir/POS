<Window x:Class="POS.Retail.LateReturnDialogForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Late_Return_Dialog_frm" Height="250" Width="453">
    <Grid Width="405">
        <TextBox Margin="12,56,0,0" Name="InputTextBox" HorizontalAlignment="Left" Width="367" Height="34" VerticalAlignment="Top" MouseDoubleClick="InputTextBox_MouseDoubleClick" KeyDown="InputTextBox_KeyDown" />
        <Button Content="OK" Margin="12,101,0,0" Name="BtnOk" Padding="8" HorizontalAlignment="Left" Width="170" Height="46" VerticalAlignment="Top" Click="BtnOk_Click" />
        <Button Content="Cancel" Margin="209,101,0,0" Name="BtnCancel" Padding="8" HorizontalAlignment="Left" Width="170" Height="46" VerticalAlignment="Top" />
        <Button Content="Keyboard" Margin="52,157,0,0" Name="btn_keyboard" HorizontalAlignment="Left" Width="301" Height="49" VerticalAlignment="Top" Click="btn_keyboard_Click" />
        <Label Content="This rental is late! Enter (I) to Invoice late charge now or&#xd;(W) to Waive late charge and press OK." Height="43" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" Width="393" />
    </Grid>
</Window>
